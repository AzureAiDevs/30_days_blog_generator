{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Welcome6",
    "definitions": {
        "Welcome6": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "campaign": {
                    "$ref": "#/definitions/Campaign"
                }
            },
            "required": [
                "campaign"
            ],
            "title": "Welcome6"
        },
        "Campaign": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "static_img_folder": {
                    "type": "string"
                },
                "static_img_path": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "daily_blog_url": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "social_tags": {
                    "type": "string"
                },
                "days": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Day"
                    }
                }
            },
            "required": [
                "daily_blog_url",
                "days",
                "name",
                "social_tags",
                "static_img_folder",
                "static_img_path"
            ],
            "title": "Campaign"
        },
        "Day": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "folder": {
                    "type": "string"
                },
                "emoji": {
                    "$ref": "#/definitions/Emoji"
                },
                "audience": {
                    "$ref": "#/definitions/Audience"
                },
                "title": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "authors": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "keywords": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "canonical": {
                    "anyOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "string",
                            "format": "uri",
                            "qt-uri-protocols": [
                                "https"
                            ],
                            "qt-uri-extensions": [
                                ".html"
                            ]
                        }
                    ]
                },
                "social_tag": {
                    "type": "string"
                },
                "references": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "audience",
                "authors",
                "canonical",
                "description",
                "emoji",
                "folder",
                "keywords",
                "tags",
                "title"
            ],
            "title": "Day"
        },
        "Audience": {
            "type": "string",
            "enum": [
                "Azure AI Developers",
                "Azure AI App Developers",
                "Azure ML Developers",
                "Azure AI Workshop"
            ],
            "title": "Audience"
        },
